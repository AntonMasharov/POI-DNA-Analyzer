<Window x:Class="POI_DNA_Analyzer.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:oxy="http://oxyplot.org/wpf"
		Title="POI-DNA-Analyzer"
		Width="800"
		Height="480"
		MinWidth="800"
		MinHeight="480"
		Icon="/icon2.png"
		WindowStyle="ThreeDBorderWindow"
		mc:Ignorable="d">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="0"/>
		</Grid.RowDefinitions>
		<Menu>
			<MenuItem Header="{DynamicResource Language}">
				<MenuItem Click="SetRULang"
						  Header="Русский"/>
				<MenuItem Click="SetENLang"
						  Header="English"/>
			</MenuItem>
		</Menu>
		<TabControl Margin="0,22,0,0">
			<TabItem x:Name="SequenceFinderTab">
				<TabItem.Header>
					<StackPanel Orientation="Horizontal">
						<TextBlock Margin="3"
								   Text="{DynamicResource SequenceFinderTab}"/>
					</StackPanel>
				</TabItem.Header>
				<Grid Margin="0,0,0,0">
					<Grid.RowDefinitions/>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="29*"/>
						<ColumnDefinition Width="24*"/>
					</Grid.ColumnDefinitions>
					<TextBlock Width="47"
							   Height="21"
							   Margin="25,45,0,0"
							   HorizontalAlignment="Left"
							   VerticalAlignment="Top"
							   Background="White"
							   FontSize="14"
							   IsEnabled="False"
							   TextWrapping="Wrap"><Run Language="ru-ru"
							 Text="Запрос:"/><LineBreak/><Run Language="ru-ru"/></TextBlock>
					<TextBox x:Name="PromptField"
							 Width="216"
							 Height="21"
							 Margin="77,45,0,0"
							 HorizontalAlignment="Left"
							 VerticalAlignment="Top"
							 Text=""
							 TextWrapping="Wrap"/>
					<Button x:Name="EnterPrompt"
							Width="42"
							Height="21"
							Margin="295,45,0,0"
							HorizontalAlignment="Left"
							VerticalAlignment="Top"
							Click="EnterPromptButtonClick"
							Content="{DynamicResource FindButton}"
							FontWeight="Bold"
							RenderTransformOrigin="-0.024,0.528"/>
					<TextBlock x:Name="ResultText"
							   Grid.Column="1"
							   Height="21"
							   Margin="24,48,44,0"
							   VerticalAlignment="Top"
							   FontFamily="Arial"
							   FontSize="14"
							   Text="{DynamicResource ResultText}"
							   TextTrimming="WordEllipsis"
							   TextWrapping="Wrap"/>
					<Button x:Name="SaveFileButton"
							Grid.Column="1"
							Width="100"
							Height="30"
							Margin="0,0,40,6"
							HorizontalAlignment="Right"
							VerticalAlignment="Bottom"
							Click="SaveFileButtonClick"
							Content="{DynamicResource SaveButton}"/>
					<Button x:Name="ClearButton"
							Width="70"
							Height="21"
							Margin="342,45,0,0"
							HorizontalAlignment="Left"
							VerticalAlignment="Top"
							Click="ClearResultButtonClick"
							Content="{DynamicResource ClearButton}"/>
					<ListBox x:Name="List"
							 Grid.Column="1"
							 Margin="24,74,44,60"
							 d:ItemsSource="{d:SampleData ItemCount=0}"/>
					<Button x:Name="SaveMenuButton"
							Grid.Column="1"
							Width="30"
							Height="30"
							Margin="0,0,10,6"
							HorizontalAlignment="Right"
							VerticalAlignment="Bottom"
							Click="SaveMenuButtonClick"
							Content="...">
						<Button.ContextMenu>
							<ContextMenu x:Name="SaveButtonContextMenu">
								<MenuItem Header="{DynamicResource SaveIndividuallyCheckbox}">
									<MenuItem.Icon>
										<CheckBox x:Name="SaveIndividuallyCheckbox"
												  IsChecked="False"
												  IsEnabled="True"/>
									</MenuItem.Icon>
								</MenuItem>
								<MenuItem Header="{DynamicResource SaveTogetherCheckbox}">
									<MenuItem.Icon>
										<CheckBox x:Name="SaveTogetherCheckbox"
												  IsChecked="True"
												  IsEnabled="True"/>
									</MenuItem.Icon>
								</MenuItem>
							</ContextMenu>
						</Button.ContextMenu>
					</Button>
				</Grid>
			</TabItem>
			<TabItem x:Name="DinucleotidesAnalyzerTab">
				<TabItem.Header>
					<StackPanel Orientation="Horizontal">
						<TextBlock Margin="3"
								   Text="{DynamicResource DinucleotidesAnalyzerTab}"/>
					</StackPanel>
				</TabItem.Header>
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="400"/>
						<ColumnDefinition Width="*"/>
					</Grid.ColumnDefinitions>
					<Grid Grid.Column="0">
						<Grid.RowDefinitions>
							<RowDefinition/>
						</Grid.RowDefinitions>
						<Grid Grid.Row="0">
							<TextBlock x:Name="ChunkSize"
									   Width="87"
									   Height="21"
									   Margin="25,45,0,0"
									   HorizontalAlignment="Left"
									   VerticalAlignment="Top"
									   FontSize="14"
									   Text="{DynamicResource ChunkSize}"
									   TextWrapping="Wrap"/>
							<TextBox x:Name="ChunkSizeTextBox"
									 Width="112"
									 Height="21"
									 Margin="77,45,0,0"
									 HorizontalAlignment="Left"
									 VerticalAlignment="Top"
									 Text="100"
									 TextWrapping="Wrap"/>
							<CheckBox x:Name="EnableSliderCheckBox"
									  Width="314"
									  Height="24"
									  Margin="25,113,0,0"
									  HorizontalAlignment="Left"
									  VerticalAlignment="Top"
									  VerticalContentAlignment="Center"
									  Content="{DynamicResource EnableSliderCheckBox}"
									  FontSize="14"
									  IsChecked="True"/>
							<Slider x:Name="SimilaritySlider"
									Width="174"
									Height="23"
									Margin="165,78,0,0"
									HorizontalAlignment="Left"
									VerticalAlignment="Top"
									IsEnabled="{Binding IsChecked, ElementName=EnableSliderCheckBox}"
									IsSnapToTickEnabled="True"
									Maximum="100"
									TickFrequency="1"
									TickPlacement="BottomRight"
									Value="80">
								<Slider.Style>
									<Style TargetType="Slider">
										<Style.Triggers>
											<Trigger Property="IsEnabled" Value="False"/>
										</Style.Triggers>
									</Style>
								</Slider.Style>
							</Slider>
							<TextBox Width="43"
									 Height="18"
									 Margin="344,81,0,0"
									 HorizontalAlignment="Left"
									 VerticalAlignment="Top"
									 Text="{Binding ElementName=SimilaritySlider, Path=Value, UpdateSourceTrigger=PropertyChanged}"
									 TextWrapping="Wrap"/>
							<TextBlock Width="140"
									   Height="22"
									   Margin="25,78,0,0"
									   HorizontalAlignment="Left"
									   VerticalAlignment="Top"
									   FontSize="14"
									   Text="{DynamicResource SimilarityCoefficient}"
									   TextWrapping="Wrap"/>
							<Grid Width="138"
								  Height="170"
								  Margin="25,142,0,0"
								  HorizontalAlignment="Left"
								  VerticalAlignment="Top">
								<Grid.RowDefinitions>
									<RowDefinition Height="27*"/>
									<RowDefinition Height="58*"/>
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="31*"/>
									<ColumnDefinition Width="38*"/>
								</Grid.ColumnDefinitions>
								<Button Width="30"
										Height="30"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Background="#FFB9B9B9"
										Click="ShowGraph"
										Content="An"
										Tag="A"/>
								<Button Grid.ColumnSpan="2"
										Width="30"
										Height="30"
										Margin="35,0,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Background="#FFB9B9B9"
										Click="ShowGraph"
										Content="Cn"
										Tag="C"/>
								<Button Grid.Column="1"
										Width="30"
										Height="30"
										Margin="8,0,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Background="#FFB9B9B9"
										Click="ShowGraph"
										Content="Tn"
										Tag="T"/>
								<Button Grid.Column="1"
										Width="30"
										Height="30"
										Margin="43,0,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Background="#FFB9B9B9"
										Click="ShowGraph"
										Content="Gn"
										Tag="G"/>
								<Button Grid.RowSpan="2"
										Width="30"
										Height="30"
										Margin="0,35,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="AA"
										Tag="AA"/>
								<Button Grid.RowSpan="2"
										Grid.ColumnSpan="2"
										Width="30"
										Height="30"
										Margin="35,35,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="CA"
										Tag="CA"/>
								<Button Grid.RowSpan="2"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="8,35,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="TA"
										Tag="TA"/>
								<Button Grid.RowSpan="2"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="43,35,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="GA"
										Tag="GA"/>
								<Button Grid.Row="1"
										Width="30"
										Height="30"
										Margin="0,16,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="AC"
										Tag="AC"/>
								<Button Grid.Row="1"
										Grid.ColumnSpan="2"
										Width="30"
										Height="30"
										Margin="35,16,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="CC"
										Tag="CC"/>
								<Button Grid.Row="1"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="8,16,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="TC"
										Tag="TC"/>
								<Button Grid.Row="1"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="43,16,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="GC"
										Tag="GC"/>
								<Button Grid.Row="1"
										Width="30"
										Height="30"
										Margin="0,51,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="AT"
										Tag="AT"/>
								<Button Grid.Row="1"
										Grid.ColumnSpan="2"
										Width="30"
										Height="30"
										Margin="35,51,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="CT"
										Tag="CT"/>
								<Button Grid.Row="1"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="8,51,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="TT"
										Tag="TT"/>
								<Button Grid.Row="1"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="43,51,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="GT"
										Tag="GT"/>
								<Button Grid.Row="1"
										Width="30"
										Height="30"
										Margin="0,86,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="AG"
										Tag="AG"/>
								<Button Grid.Row="1"
										Grid.ColumnSpan="2"
										Width="30"
										Height="30"
										Margin="35,86,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="CG"
										Tag="CG"/>
								<Button Grid.Row="1"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="8,86,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="TG"
										Tag="TG"/>
								<Button Grid.Row="1"
										Grid.Column="1"
										Width="30"
										Height="30"
										Margin="43,86,0,0"
										HorizontalAlignment="Left"
										VerticalAlignment="Top"
										Click="ShowGraph"
										Content="GG"
										Tag="GG"/>
							</Grid>
						</Grid>
					</Grid>
					<Grid Grid.Column="1">
						<Grid.RowDefinitions>
							<RowDefinition Height="*"/>
							<RowDefinition Height="*"/>
						</Grid.RowDefinitions>
						<Grid Grid.RowSpan="2"
							  Margin="0,204,0,0">
							<Button x:Name="SaveDinucleotidesAnalyzer"
									Width="100"
									Height="30"
									Margin="0,0,40,6"
									HorizontalAlignment="Right"
									VerticalAlignment="Bottom"
									Click="SaveDinucleotidesAnalyzerButtonClick"
									Content="{DynamicResource SaveButton}"/>
							<Button Name="SaveMenuButton2"
									Width="30"
									Height="30"
									Margin="0,0,10,6"
									HorizontalAlignment="Right"
									VerticalAlignment="Bottom"
									Click="SaveMenuButtonClick"
									Content="..."/>
							<Button x:Name="StartButton"
									Width="110"
									Height="30"
									Margin="0,0,145,6"
									HorizontalAlignment="Right"
									VerticalAlignment="Bottom"
									Click="StartDinucleotidesAnalyzerButtonClick"
									Content="{DynamicResource StartButton}"
									FontSize="14"
									FontWeight="Bold"/>
							<ScrollBar x:Name="HorizontalScrollBar"
									   Height="17"
									   Margin="7,0,13,0"
									   VerticalAlignment="Center"
									   Orientation="Horizontal"/>
							<!--<ProgressBar x:Name="DinucleotidesAnalyzerProgressBar"
								Margin="10,0,202,10"
								Height="29"
								VerticalAlignment="Bottom"/>-->
						</Grid>
						<Grid Grid.Row="0">
							<oxy:PlotView x:Name="OxyPlot"
										  Margin="0,66,0,-94"
										  Model="{Binding SomeName}"/>
						</Grid>
					</Grid>
				</Grid>
			</TabItem>
			<TabItem x:Name="OpenReadingFrameTab">
				<TabItem.Header>
					<StackPanel Orientation="Horizontal">
						<TextBlock Margin="3"
								   Text="{DynamicResource OpenReadingFrameTab}"/>
					</StackPanel>
				</TabItem.Header>
				<Grid>
					<Grid Height="385"
						  Margin="-1,0,0,0">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
						</Grid.ColumnDefinitions>
						<Button Name="CreateComplementaryDNA"
								Width="130"
								Height="35"
								Margin="68,77,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="CreateComplementaryDNAButtonClick"
								Content="{DynamicResource StartButton}"
								FontSize="13"/>
						<Button Width="94"
								Height="35"
								Margin="68,120,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="SaveComplementaryDNAFileButtonClick"
								Content="{DynamicResource SaveButton}"/>
						<Button Name="SaveMenuButton6"
								Grid.Column="0"
								Width="36"
								Margin="162,120,0,230"
								HorizontalAlignment="Left"
								Click="SaveMenuButtonClick"
								Content="..."
								RenderTransformOrigin="0.5,0.5">
							<Button.RenderTransform>
								<TransformGroup>
									<ScaleTransform ScaleX="-1"/>
									<SkewTransform/>
									<RotateTransform/>
									<TranslateTransform/>
								</TransformGroup>
							</Button.RenderTransform>
						</Button>
						<TextBlock Width="230"
								   Height="28"
								   Margin="18,40,0,0"
								   HorizontalAlignment="Left"
								   VerticalAlignment="Top"
								   FontSize="14"
								   FontWeight="Bold"
								   Text="{DynamicResource MakeComplementaryDNA}"
								   TextAlignment="Center"
								   TextWrapping="Wrap"/>
						<Button Grid.Column="1"
								Width="130"
								Height="35"
								Margin="67,100,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="StartTranslation"
								Content="{DynamicResource StartButton}"
								FontSize="13"/>
						<Button Grid.Column="1"
								Width="130"
								Height="34"
								Margin="67,143,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="ChangeTranslationResultPath"
								Content="{DynamicResource ChooseSavePath}"/>
						<Button Grid.Column="1"
								Width="96"
								Height="35"
								Margin="67,186,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="ChangeTranslationConfig"
								Content="{DynamicResource ChangeConfig}"/>
						<Button Grid.Column="1"
								Width="35"
								Height="35"
								Margin="163,186,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="ResetTranslationConfigButtonClick"
								Content="R"/>
						<TextBlock Grid.Column="1"
								   Width="170"
								   Height="35"
								   Margin="47,63,0,0"
								   HorizontalAlignment="Left"
								   VerticalAlignment="Top"
								   FontSize="14"
								   FontWeight="Bold"
								   Text="{DynamicResource Translate}"
								   TextAlignment="Center"
								   TextWrapping="Wrap"/>
						<Button Grid.Column="2"
								Width="130"
								Height="35"
								Margin="68,120,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="OpenReadingFrameStartButtonClick"
								Content="{DynamicResource StartButton}"
								FontSize="13"/>
						<Button Grid.Column="2"
								Width="130"
								Height="35"
								Margin="68,163,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="ChangeOpenReadingFramesResultPath"
								Content="{DynamicResource ChooseSavePath}"/>
						<TextBlock Grid.Column="2"
								   Width="245"
								   Height="35"
								   Margin="10,75,0,0"
								   HorizontalAlignment="Left"
								   VerticalAlignment="Top"
								   FontSize="14"
								   FontWeight="Bold"
								   Text="{DynamicResource FindOpenReadingFrames}"
								   TextAlignment="Center"
								   TextWrapping="Wrap"/>
						<TextBox Name="MinSizeToSaveTextBox"
								 Grid.Column="2"
								 Width="120"
								 Margin="72,285,0,0"
								 HorizontalAlignment="Left"
								 VerticalAlignment="Top"
								 Text="100"
								 TextWrapping="Wrap"/>
						<TextBlock Grid.Column="2"
								   Width="199"
								   Height="31"
								   MaxWidth="200"
								   Margin="33,250,0,0"
								   HorizontalAlignment="Left"
								   VerticalAlignment="Top"
								   Text="{DynamicResource OpenReadingFrameMinSizeToSave}"
								   TextAlignment="Center"
								   TextWrapping="Wrap"/>
						<Button Grid.Column="2"
								Width="94"
								Height="35"
								Margin="68,206,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="ChangeOpenReadingFramesConfig"
								Content="{DynamicResource ChangeConfig}"/>
						<Button Grid.Column="2"
								Width="36"
								Height="35"
								Margin="162,206,0,0"
								HorizontalAlignment="Left"
								VerticalAlignment="Top"
								Click="ResetOpenReadingFramesConfigButtonClick"
								Content="R"/>
					</Grid>
					<Button x:Name="CreateEverything"
							Width="79"
							Height="30"
							Margin="0,0,10,6"
							HorizontalAlignment="Right"
							VerticalAlignment="Bottom"
							Click="CreateEverythingButtonClick"
							Content="{DynamicResource OpenReadingFrameStartEverything}"/>
					<Button x:Name="ChooseSavePath"
							Width="100"
							Height="30"
							Margin="0,0,94,6"
							HorizontalAlignment="Right"
							VerticalAlignment="Bottom"
							Click="ChooseOpenReadingFramesSavePathButtonClick"
							Content="{DynamicResource ChooseSavePath}"/>
				</Grid>
			</TabItem>
			<TabItem x:Name="ATGCWindow">
				<TabItem.Header>
					<StackPanel Orientation="Horizontal">
						<TextBlock Margin="3"
								   Text="{DynamicResource ATGCPercent}"/>
					</StackPanel>
				</TabItem.Header>
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="200"/>
						<ColumnDefinition Width="*"/>
					</Grid.ColumnDefinitions>
					<Button x:Name="StartATGCPercentButton"
							Width="110"
							Height="30"
							Margin="25,45,0,0"
							HorizontalAlignment="Left"
							VerticalAlignment="Top"
							Click="StartATGCPercentButtonClick"
							Content="{DynamicResource StartButton}"
							FontWeight="Bold"/>
					<Button x:Name="SaveATGCPercent"
							Grid.Column="1"
							Width="100"
							Height="30"
							Margin="0,0,40,6"
							HorizontalAlignment="Right"
							VerticalAlignment="Bottom"
							Click="SaveATGCPercentButtonClick"
							Content="{DynamicResource SaveButton}"/>
					<Button Name="SaveMenuButton5"
							Grid.Column="1"
							Width="30"
							Height="30"
							Margin="0,0,10,6"
							HorizontalAlignment="Right"
							VerticalAlignment="Bottom"
							Click="SaveMenuButtonClick"
							Content="..."
							RenderTransformOrigin="0.519,0.577"/>
					<TextBlock Name="ATGCResult"
							   Width="142"
							   Height="72"
							   Margin="25,117,0,0"
							   HorizontalAlignment="Left"
							   VerticalAlignment="Top"
							   FontSize="14"
							   Text="Результат:"
							   TextAlignment="Center"
							   TextWrapping="Wrap"/>
					<TextBlock x:Name="ATCGChunkSize"
							   Width="94"
							   Height="21"
							   Margin="25,85,0,0"
							   HorizontalAlignment="Left"
							   VerticalAlignment="Top"
							   FontSize="14"
							   Text="{DynamicResource ChunkSize}"
							   TextWrapping="Wrap"/>
					<TextBox x:Name="ATCGChunkSizeTextBox"
							 Width="63"
							 Height="21"
							 Margin="124,85,0,0"
							 HorizontalAlignment="Left"
							 VerticalAlignment="Top"
							 Text="100"
							 TextWrapping="Wrap"/>
					<Grid Grid.Column="1"
						  Margin="0,30,0,50">
						<Grid.RowDefinitions>
							<RowDefinition Height="0*"/>
							<RowDefinition/>
						</Grid.RowDefinitions>
						<oxy:PlotView x:Name="OxyPlotATCGPercent"
									  Grid.Row="1"
									  Margin="10,1,10,22"
									  Model="{Binding SomeName}"/>
						<ScrollBar x:Name="OxyPlotATCGPercentHorizontalScrollBar"
								   Grid.Row="1"
								   Height="17"
								   Margin="10,0,10,2"
								   VerticalAlignment="Bottom"
								   Orientation="Horizontal"/>
					</Grid>
				</Grid>
